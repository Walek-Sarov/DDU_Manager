			<dataPath>ГруппаСотрудников</dataPath>
			<field>ГруппаСотрудников</field>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>ДатаПриемаНаРаботу</dataPath>
			<field>ДатаПриемаНаРаботу</field>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>ВидДокумента</dataPath>
			<field>ВидДокумента</field>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>ВидСтажа</dataPath>
			<field>ВидСтажа</field>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>НомерДокумента</dataPath>
			<field>НомерДокумента</field>
