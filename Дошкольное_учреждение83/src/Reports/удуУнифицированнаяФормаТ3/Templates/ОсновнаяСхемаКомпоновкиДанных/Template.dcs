		<field xsi:type="DataSetFieldField">
			<dataPath>ФИОКадровика</dataPath>
			<field>ФИОКадровика</field>
			<title xsi:type="v8:LocalStringType">
				<v8:item>
					<v8:lang>ru</v8:lang>
					<v8:content>ФИОКадровика</v8:content>
				</v8:item>
			</title>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>ФИОРуководителя</dataPath>
			<field>ФИОРуководителя</field>
			<title xsi:type="v8:LocalStringType">
				<v8:item>
					<v8:lang>ru</v8:lang>
					<v8:content>ФИОРуководителя</v8:content>
				</v8:item>
			</title>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>ДолжностьРуководителя</dataPath>
			<field>ДолжностьРуководителя</field>
			<title xsi:type="v8:LocalStringType">
				<v8:item>
					<v8:lang>ru</v8:lang>
					<v8:content>Должность руководителя</v8:content>
				</v8:item>
			</title>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>Порядок</dataPath>
			<field>Порядок</field>
			<title xsi:type="v8:LocalStringType">
				<v8:item>
					<v8:lang>ru</v8:lang>
					<v8:content>Порядок</v8:content>
				</v8:item>
			</title>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>ДолжностьКадровика</dataPath>
			<field>ДолжностьКадровика</field>
			<title xsi:type="v8:LocalStringType">
				<v8:item>
					<v8:lang>ru</v8:lang>
					<v8:content>Должность кадровика</v8:content>
				</v8:item>
			</title>
		</field>
		<dataSource>ИнформационнаяБаза</dataSource>
		<query>ВЫБРАТЬ РАЗРЕШЕННЫЕ
	ДанныеСНадбавками.Порядок КАК Порядок,
	ДанныеСНадбавками.ПодразделениеОрганизации КАК ПодразделениеОрганизации,
	ДанныеСНадбавками.Должность КАК Должность,
	ДанныеСНадбавками.КоличествоСтавок КАК КоличествоСтавок,
	ДанныеСНадбавками.МинСтавка КАК МинСтавка,
	ДанныеСНадбавками.МинОсновнойТариф КАК МинОсновнойТариф,
	ДанныеСНадбавками.ВидНадбавки КАК ВидНадбавки,
	ДанныеСНадбавками.РазмерНадбавки КАК РазмерНадбавки,
	ДанныеСНадбавками.СпособРасчета КАК СпособРасчета,
	ВЫРАЗИТЬ(ДанныеСНадбавками.КоличествоСтавок * ДанныеСНадбавками.РазмерНадбавки КАК ЧИСЛО(15, 2)) КАК МинСуммаНадбавки,
	ДанныеСНадбавками.Организация КАК Организация,
	ЕСТЬNULL(ВЫБОР
			КОГДА ФИОРуководителя.Имя = ""
				ТОГДА ""
			ИНАЧЕ ПОДСТРОКА(ФИОРуководителя.Имя, 1, 1) + ". "
		КОНЕЦ + ВЫБОР
			КОГДА ФИОРуководителя.Отчество = ""
				ТОГДА ""
			ИНАЧЕ ПОДСТРОКА(ФИОРуководителя.Отчество, 1, 1) + ". "
		КОНЕЦ + ФИОРуководителя.Фамилия, Руководитель.ФизЛицо.Наименование) КАК ФИОРуководителя,
	ЕСТЬNULL(ВЫБОР
			КОГДА ФИОКАдровика.Имя = ""
				ТОГДА ""
			ИНАЧЕ ПОДСТРОКА(ФИОКАдровика.Имя, 1, 1) + ". "
		КОНЕЦ + ВЫБОР
			КОГДА ФИОКАдровика.Отчество = ""
				ТОГДА ""
			ИНАЧЕ ПОДСТРОКА(ФИОКАдровика.Отчество, 1, 1) + ". "
		КОНЕЦ + ФИОКАдровика.Фамилия, ФИОКАдровика.ФизЛицо.Наименование) КАК ФИОКадровика,
	ЕСТЬNULL(ВЫБОР
			КОГДА ФИОГлавбуха.Имя = ""
				ТОГДА ""
			ИНАЧЕ ПОДСТРОКА(ФИОГлавбуха.Имя, 1, 1) + ". "
		КОНЕЦ + ВЫБОР
			КОГДА ФИОГлавбуха.Отчество = ""
				ТОГДА ""
			ИНАЧЕ ПОДСТРОКА(ФИОГлавбуха.Отчество, 1, 1) + ". "
		КОНЕЦ + ФИОГлавбуха.Фамилия, ГлавБух.ФизЛицо.Наименование) КАК ФИОГлавбуха,
	Руководитель.Должность КАК ДолжностьРуководителя,
	КАдровик.Должность КАК ДолжностьКадровика
{ВЫБРАТЬ
	Порядок,
	ПодразделениеОрганизации.*,
	Должность.*,
	КоличествоСтавок,
	МинСтавка,
	МинОсновнойТариф,
	ВидНадбавки.*,
	РазмерНадбавки,
	СпособРасчета.*,
	МинСуммаНадбавки,
	Организация.*,
	ФИОРуководителя,
	ФИОКадровика,
	ФИОГлавбуха,
	ДолжностьРуководителя,
	ДолжностьКадровика.*}
ИЗ
	(ВЫБРАТЬ
		Расписание.ПодразделениеОрганизации КАК ПодразделениеОрганизации,
		Расписание.Должность КАК Должность,
		Расписание.КоличествоСтавок КАК КоличествоСтавок,
		ВЫРАЗИТЬ(Расписание.МесячнаяСтавка КАК ЧИСЛО(15, 2)) КАК МинСтавка,
		ВЫРАЗИТЬ(Расписание.КоличествоСтавок * Расписание.МесячнаяСтавка КАК ЧИСЛО(15, 2)) КАК МинОсновнойТариф,
		ИспользуемыеНадбавки.ВидНадбавки КАК ВидНадбавки,
		ВЫБОР
			КОГДА ИспользуемыеНадбавки.ВидНадбавки = ЗНАЧЕНИЕ(ПланВидовРасчета.удуОсновныеНачисленияОрганизаций.СевернаяНадбавка)
				ТОГДА Расписание.СевернаяНадбавка
			КОГДА ИспользуемыеНадбавки.ВидНадбавки = ЗНАЧЕНИЕ(ПланВидовРасчета.удуОсновныеНачисленияОрганизаций.РайонныйКоэффициент)
				ТОГДА Расписание.НадбавкаПоРайонномуКоэффициенту
			ИНАЧЕ НадбавкиПоШтатномуРасписанию.РазмерНадбавки
		КОНЕЦ КАК РазмерНадбавки,
		НадбавкиПоШтатномуРасписанию.ВидНадбавки.СпособРасчета КАК СпособРасчета,
		Расписание.ПодразделениеОрганизации.Организация КАК Организация,
		Расписание.Порядок КАК Порядок
	ИЗ
		РегистрСведений.удуШтатноеРасписаниеОрганизаций.СрезПоследних(&amp;Период, ) КАК Расписание
			ЛЕВОЕ СОЕДИНЕНИЕ (ВЫБРАТЬ РАЗЛИЧНЫЕ
				НадбавкиПоШтатномуРасписаниюОрганизацииСрезПоследних.ВидНадбавки КАК ВидНадбавки,
				НадбавкиПоШтатномуРасписаниюОрганизацииСрезПоследних.ПодразделениеОрганизации.Организация КАК Организация
			ИЗ
				РегистрСведений.удуНадбавкиПоШтатномуРасписаниюОрганизаций.СрезПоследних(&amp;Период, ) КАК НадбавкиПоШтатномуРасписаниюОрганизацииСрезПоследних
			
			ОБЪЕДИНИТЬ
			
			ВЫБРАТЬ ПЕРВЫЕ 1
				ЗНАЧЕНИЕ(ПланВидовРасчета.удуОсновныеНачисленияОрганизаций.СевернаяНадбавка),
				удуУчреждения.Ссылка
			ИЗ
				Справочник.удуУчреждения КАК удуУчреждения
			
			ОБЪЕДИНИТЬ
			
			ВЫБРАТЬ ПЕРВЫЕ 1
				ЗНАЧЕНИЕ(ПланВидовРасчета.удуОсновныеНачисленияОрганизаций.РайонныйКоэффициент),
				удуУчреждения.Ссылка
			ИЗ
				Справочник.удуУчреждения КАК удуУчреждения) КАК ИспользуемыеНадбавки
			ПО Расписание.ПодразделениеОрганизации.Организация = ИспользуемыеНадбавки.Организация
			ЛЕВОЕ СОЕДИНЕНИЕ (ВЫБРАТЬ
				НадбавкиПоШтатномуРасписанию.ПодразделениеОрганизации КАК ПодразделениеОрганизации,
				НадбавкиПоШтатномуРасписанию.Должность КАК Должность,
				НадбавкиПоШтатномуРасписанию.ВидНадбавки КАК ВидНадбавки,
				НадбавкиПоШтатномуРасписанию.МесячныйРазмер КАК РазмерНадбавки,
				НадбавкиПоШтатномуРасписанию.ПодразделениеОрганизации.Организация КАК Организация
			ИЗ
				РегистрСведений.удуНадбавкиПоШтатномуРасписаниюОрганизаций.СрезПоследних(&amp;Период, ) КАК НадбавкиПоШтатномуРасписанию
			
			ОБЪЕДИНИТЬ ВСЕ
			
			ВЫБРАТЬ
				ШтатноеРасписаниеОрганизацийСрезПоследних.ПодразделениеОрганизации,
				ШтатноеРасписаниеОрганизацийСрезПоследних.Должность,
				ЗНАЧЕНИЕ(ПланВидовРасчета.удуОсновныеНачисленияОрганизаций.СевернаяНадбавка),
				ШтатноеРасписаниеОрганизацийСрезПоследних.СевернаяНадбавка,
				Организации.Ссылка
			ИЗ
				РегистрСведений.удуШтатноеРасписаниеОрганизаций.СрезПоследних(&amp;Период, ) КАК ШтатноеРасписаниеОрганизацийСрезПоследних
					ЛЕВОЕ СОЕДИНЕНИЕ Справочник.удуУчреждения КАК Организации
					ПО ШтатноеРасписаниеОрганизацийСрезПоследних.ПодразделениеОрганизации.Организация = Организации.Ссылка
			
			ОБЪЕДИНИТЬ ВСЕ
			
			ВЫБРАТЬ
				ШтатноеРасписаниеОрганизацийСрезПоследних.ПодразделениеОрганизации,
				ШтатноеРасписаниеОрганизацийСрезПоследних.Должность,
				ЗНАЧЕНИЕ(ПланВидовРасчета.удуОсновныеНачисленияОрганизаций.РайонныйКоэффициент),
				ШтатноеРасписаниеОрганизацийСрезПоследних.НадбавкаПоРайонномуКоэффициенту,
				Организации.Ссылка
			ИЗ
				РегистрСведений.удуШтатноеРасписаниеОрганизаций.СрезПоследних(&amp;Период, ) КАК ШтатноеРасписаниеОрганизацийСрезПоследних
					ЛЕВОЕ СОЕДИНЕНИЕ Справочник.удуУчреждения КАК Организации
					ПО ШтатноеРасписаниеОрганизацийСрезПоследних.ПодразделениеОрганизации.Организация = Организации.Ссылка) КАК НадбавкиПоШтатномуРасписанию
			ПО (НадбавкиПоШтатномуРасписанию.ВидНадбавки = ИспользуемыеНадбавки.ВидНадбавки)
				И (НадбавкиПоШтатномуРасписанию.ПодразделениеОрганизации = Расписание.ПодразделениеОрганизации)
				И (НадбавкиПоШтатномуРасписанию.Должность = Расписание.Должность)
				И Расписание.ПодразделениеОрганизации.Организация = НадбавкиПоШтатномуРасписанию.Организация
	ГДЕ
		Расписание.КоличествоСтавок &gt; 0) КАК ДанныеСНадбавками
		ЛЕВОЕ СОЕДИНЕНИЕ РегистрСведений.удуОтветственныеЛицаУчреждения.СрезПоследних(&amp;Период, ) КАК Руководитель
			ЛЕВОЕ СОЕДИНЕНИЕ РегистрСведений.удуФИОФизЛиц.СрезПоследних(&amp;Период, ) КАК ФИОРуководителя
			ПО Руководитель.ФизЛицо = ФИОРуководителя.ФизЛицо
		ПО (Руководитель.Учреждение = ДанныеСНадбавками.ПодразделениеОрганизации.Организация)
			И (Руководитель.ОтветственноеЛицо = ЗНАЧЕНИЕ(Перечисление.ОтветственныеЛицаОрганизаций.РуководительОрганизации))
		ЛЕВОЕ СОЕДИНЕНИЕ РегистрСведений.удуОтветственныеЛицаУчреждения.СрезПоследних(&amp;Период, ) КАК ГлавБух
			ЛЕВОЕ СОЕДИНЕНИЕ РегистрСведений.удуФИОФизЛиц.СрезПоследних(&amp;Период, ) КАК ФИОГлавбуха
			ПО ГлавБух.ФизЛицо = ФИОГлавбуха.ФизЛицо
		ПО (ГлавБух.Учреждение = ДанныеСНадбавками.ПодразделениеОрганизации.Организация)
			И (ГлавБух.ОтветственноеЛицо = ЗНАЧЕНИЕ(Перечисление.ОтветственныеЛицаОрганизаций.ГлавныйБухгалтер))
		ЛЕВОЕ СОЕДИНЕНИЕ РегистрСведений.удуОтветственныеЛицаУчреждения.СрезПоследних(&amp;Период, ) КАК КАдровик
			ЛЕВОЕ СОЕДИНЕНИЕ РегистрСведений.удуФИОФизЛиц.СрезПоследних(&amp;Период, ) КАК ФИОКАдровика
			ПО КАдровик.ФизЛицо = ФИОКАдровика.ФизЛицо
		ПО (КАдровик.Учреждение = ДанныеСНадбавками.ПодразделениеОрганизации.Организация)
